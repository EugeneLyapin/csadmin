                        onchange="this.value=this.value.replace(/([^0-9])/g,'');"
                        onkeyup="var n=this.value.replace(/([^0-9])/g,'');
                        if(n!=this.value) this.value=n;"
                        onmousedown="this.value=this.value.replace(/([^0-9])/g,'');"
